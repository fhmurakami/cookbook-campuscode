<% if cuisine.errors.any? %>
  <% cuisine.errors.full_messages.each do |message| %>
    <p><%= message %></p>
  <% end %>
<% end %>

<%= form_with model: cuisine, local:true, class: 'mt-4' do |f| %>
  <div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name, class: 'form-control col-md-6' %>
  </div>
  <%= f.submit t('.submit'), class: 'btn btn-primary', id: 'commit' %>
<% end %>